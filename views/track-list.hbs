<h1 class="page-title">Album {{this.albumTitle}}</h1>
<div class="track-container">
    {{#each results}}
        <div class="track">
            <h3>{{this.name}}</h3>
            <audio
                controls
                src="{{this.preview_url}}">
                    <a href="{{this.preview_url}}">
                        Download audio
                    </a>
            </audio>
        </div>
    {{/each}}
</div>
